# Useful aliases
alias ga='git add'
alias gaa='git add .'
alias gcm='git commit -s -m'
alias gc='git commit'
alias gs='git status'
alias gco='git checkout'
alias gcob='git checkout -b'
alias gp='git push'
alias gpl='git pull'
alias gd='git diff'
alias gl='git log'
alias grl='git reflog'
alias gcl='git clone'
alias gsq='git rebase -i HEAD~'

alias nis='npm install --save'
alias nisd='npm install --save-dev'
alias ni='npm install'

alias change='vim ~/.zshrc'
alias use='source ~/.zshrc'
